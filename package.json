  "description": "Manage component state in storybook stories. Fork for storybook v4",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && cd src/ && babel index.js register.js --out-dir ../dist",
